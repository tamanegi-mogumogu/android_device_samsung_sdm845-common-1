# Device specific property overrides

# Surfaceflinger
debug.sf.latch_unsignaled=1
ro.surface_flinger.max_frame_buffer_acquired_buffers=3
ro.surface_flinger.supports_background_blur=1
ro.sf.blurs_are_expensive=1

# Telephony
ro.telephony.default_network=10,10


# Process pool
persist.device_config.runtime_native.usap_pool_enabled=true

# Tones
ro.config.vc_call_vol_steps=7

# Google Photos
debug.photos.eraser_camo=1
debug.photos.eraser_suggestion=1
debug.photos.force_pixel_eol=1
debug.photos.p_editr.eraser=1

# Set the sample rate for the BT device
persist.bluetooth.samsung_samplerate.supported=true
# IMS
ro.telephony.block_binder_thread_on_incoming_calls=false

# LMKD
ro.lmk.low=1001
ro.lmk.medium=800
ro.lmk.critical=0
ro.lmk.critical_upgrade=false
ro.lmk.upgrade_pressure=100
ro.lmk.downgrade_pressure=100
ro.lmk.kill_heaviest_task=true
ro.lmk.kill_timeout_ms=100
ro.lmk.use_minfree_levels=true
ro.lmk.log_stats=true
